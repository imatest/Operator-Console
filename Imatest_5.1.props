<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <IT_INSTALL_ROOT>C:\Program Files\Imatest\v5.1\IT</IT_INSTALL_ROOT>
    <MATLAB_RUNTIME_ROOT>C:\Program Files\MATLAB\MATLAB Runtime\v93</MATLAB_RUNTIME_ROOT>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="IT_INSTALL_ROOT">
      <Value>$(IT_INSTALL_ROOT)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="MATLAB_RUNTIME_ROOT">
      <Value>$(MATLAB_RUNTIME_ROOT)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>